{
  "type": "commonjs",
  "name": "new-folder",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "format": "prettier --config .prettierrc.json --write .",
    "lint": "eslint --ext .ts './src/**/*'",
    "jasmine": "ts-node node_modules/jasmine/bin/jasmine",
    "pretest": "db-migrate --env test reset && db-migrate --env test up",
    "test": "env MODE=test npm run jasmine",
    "watch": "nodemon ./src/server.ts",
    "db-prod-up": "db-migrate --env prod up",
    "db-prod-reset": "db-migrate --env prod reset",
    "build": "rm -rf ./dist &&  tsc --project tsconfig.json",
    "start": "node ./dist/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.3.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.14",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.3.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0"
  }
}
